{
  "builds": [
    {
      "src": "grwwebsite/package.json",  // Points to the package.json inside the nested grwwebsite folder
      "use": "@vercel/next"              // Use the Next.js builder to build your app
    }
  ],
  "routes": [
    {
      "src": "/(.*)",                      // Matches all incoming requests
      "dest": "grwwebsite/$1"              // Routes all requests to the inner grwwebsite folder
    }
  ]
}
